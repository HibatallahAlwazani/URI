#include <iostream>
 
using namespace std;
 
int main() {
 
  	int tc;
	cin>>tc;
	char x[3];
	int a, b;
	for (int i = 0; i < tc; i++)
	{
		for (int j = 0; j < 3; j++)
			cin >> x[j];
	
			if (x[0] == x[2])
			{
				a = (x[0]-'0') * (x[2]-'0');
				cout << a<< endl;
			}
			else if (isupper(x[1]))
		
			{
				a = (x[2]-'0') -( x[0]-'0');
				cout << a << endl;
			}
		
		else {
			a = x[2]-'0' +x[0]-'0';
			cout << a << endl;
		}


	}
 
    return 0;
}
