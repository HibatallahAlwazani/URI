#include <iostream>
 
using namespace std;
 
int main() {
 
    int t;
cin>>t;
for(int i=0; i<t; i++)
{
  string m;
  int c;
  cin>>m;
  cin>>c;
  for(int i=0; i<m.size();i++)
  {for(int j=0; j<c; j++)
  {
      if(m[i]=='A')
      m[i]='Z';
      else
      m[i]--;
      
     
  }
  }
  cout<<m<<endl;
}
 
    return 0;
}
