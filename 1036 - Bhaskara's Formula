#include <iostream>
 #include <cmath>
 #include <iomanip>
using namespace std;
 
int main() {

double a, b,c,r1, r2;
cin>>a>>b>>c;
if(a==0 || (b*b)<=(4*a*c))
cout<<"Impossivel calcular"<<endl;
else 
{r1= (-b+sqrt(b*b-4*a*c))/(2*a);
r2= (-b-sqrt(b*b-4*a*c))/(2*a);
cout<<setprecision(5)<<fixed<< "R1 = "<<r1<<endl;
cout<< setprecision(5)<<fixed<< "R2 = "<<r2<<endl;

}
    return 0;
}
