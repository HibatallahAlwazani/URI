#include <iostream>
#include <cmath>
using namespace std;
 
int main() {
 
    int t, x;
bool m;
cin>>t;
for(int i=0; i<t;i++)
{
    m=true;
    cin>>x;
    for(int i=2; i<=sqrt(x) && m;i++)
    {
        if(x%i==0)
       { m=false;}
    }
    if(m)
    cout<<x<<" eh primo"<<endl;
    else
    cout<<x<<" nao eh primo"<<endl;
    
    
}
 
    return 0;
}
